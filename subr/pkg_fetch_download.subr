#
# set +o errexit -o noglob is assumed.
#

pkg_fetch_download() {
	if [ -n "${PKG_URL}" ]; then
		rtl_fetch_url_wget "${PKG_URL}" "${PKG_SHA256SUM}" "${PKG_FNAME}" || return "${?}";
	fi;
	if [ -n "${PKG_URLS_GIT}" ]; then
		rtl_fetch_urls_git "${PKG_BASE_DIR}" ${PKG_URLS_GIT} || return "${?}";
	fi;
};

