#
# set +o errexit -o noglob -o nounset is assumed.
#

pkg_fetch_download() {
	if [ -n "${PKG_URL:-}" ]; then
		if ! rtl_fetch_url_wget "${PKG_URL}" "${PKG_SHA256SUM}" "${PKG_FNAME}" "${PKG_NAME}" "${PKG_MIRRORS}"; then
			return 1;
		fi;
	fi;
	if [ -n "${PKG_URLS_GIT:-}" ]; then
		if ! rtl_fetch_urls_git "${PKG_BASE_DIR}" ${PKG_URLS_GIT}; then
			return 1;
		fi;
	fi;
};

