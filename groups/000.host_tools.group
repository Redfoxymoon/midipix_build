: ${PKG_AUTOMAKE_HOST_SHA256SUM:=5d05bb38a23fd3312b10aea93840feec685bdf4a41146e78882848165d3ae921};
: ${PKG_AUTOMAKE_HOST_VERSION:=1.16.1};
