HOST_TOOLS_PACKAGES="
autoconf_host automake_host gettext_tiny_host install_strip_host
libtool_host m4_host pkgconf_host slibtool_host";
HOST_TOOLS_PREFIX="${PREFIX}";
: ${PKG_AUTOCONF_HOST_DEPENDS:="m4_host pkgconf_host slibtool_host"};
